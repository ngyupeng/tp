@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyAddressBook" as ReadOnlyAddressBook
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class AddressBook
Class ModelManager
Class UserPrefs

Class UniquePersonList
Class UniqueEventList
Class Person
Class Event
Class Address
Class Email
Class Name
Class Phone
Class Tag
Class EnrollmentYear
Class EmergencyContact
Class Role
Class EName
Class EDuration
Class EDescription
Class EAttendance

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

AddressBook .up.|> ReadOnlyAddressBook

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyAddressBook
ModelManager -left-> "1" AddressBook
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

AddressBook *--> "1" UniquePersonList
AddressBook *--> "1" UniqueEventList
UniquePersonList --> "~* all" Person
UniqueEventList ---> "~* all\n" Event
Person *--> Name
Person *--> Phone
Person *--> Email
Person *--> Address
Person *--> "0..5" Tag
Person *--> "0..1" EnrollmentYear
Person *--> "0..1" EmergencyContact
Person *--> "0..3" Role
Event *-left-> EName
Event *-left-> EDuration
Event *-left-> EDescription
Event *-left-> EAttendance

Person -[hidden]up--> I
UniquePersonList -[hidden]right-> I

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

EName -[hidden]down-> EDuration
EDuration -[hidden]down-> EDescription
EDescription -[hidden]down-> EAttendance

ModelManager --> "~* filtered" Person
ModelManager --> "  ~* filtered" Event
@enduml
